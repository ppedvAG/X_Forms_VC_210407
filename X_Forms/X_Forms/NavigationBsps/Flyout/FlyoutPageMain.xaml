<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="X_Forms.NavigationBsps.Flyout.FlyoutPageMain"
            xmlns:main="clr-namespace:X_Forms"
            xmlns:local="clr-namespace:X_Forms.NavigationBsps.Flyout">
    
    <FlyoutPage.Flyout>
        <local:FlyoutMenu x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <main:MainPage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    
</FlyoutPage>